{
  "name": "templatestatic",
  "version": "1.0.0",
  "description": "Template Static Build",
  "main": "gulpfile.babel.js",
  "dependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.11.6",
    "browser-sync": "^2.13.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-cache": "^0.4.5",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^1.3.0",
    "gulp-jshint": "^2.0.1",
    "gulp-notify": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^8.1.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-ruby-sass": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "jshint": "^2.9.2",
    "time-require": "^0.1.2",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "gulp-chmod": "^2.0.0",
    "run-sequence": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "/.git"
  },
  "keywords": [],
  "author": "Adam Taylor",
  "license": "ISC",
  "globs": {
    "styles": {
      "in": "src/css/*.scss",
      "out": "dist/css",
      "watch": "src/css/**/*.scss"
    },
    "scripts": {
      "in": "src/js",
      "out": "dist/js",
      "watch": "src/js/**/*.js"
    },
    "createRevisions": {
      "in": [ "dist/**/*.min.css", "dist/**/*.min.js" ],
      "out": "dist/"
    }
  }
}
